{
    "name": "webinarium/datatables-bundle",
    "description": "DataTables Symfony bundle",
    "type": "symfony-bundle",
    "keywords": [
        "datatable",
        "datatables"
    ],
    "homepage": "https://github.com/webinarium/DataTablesBundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "psr/log": "^1.1|^2.0",
        "symfony/config": "^2.7|^3.0|^4.0|^5.0|^6.0",
        "symfony/dependency-injection": "^2.7|^3.4.26|^4.0|^5.0|^6.0",
        "symfony/http-foundation": "^2.7|^3.4.35|^4.0|^5.0|^6.0",
        "symfony/http-kernel": "^2.7|^3.4.35|^4.0|^5.0|^6.0",
        "symfony/validator": "^2.7|^3.0|^4.0|^5.0|^6.0",
        "symfony/yaml": "^2.7|^3.0|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.0",
        "doctrine/cache": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^5.6|^6.0|^7.0|^8.0|^9.0",
        "roave/security-advisories": "dev-master",
        "symfony/cache": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "DataTables\\": [
                "src/",
                "tests/"
            ]
        }
    }
}
